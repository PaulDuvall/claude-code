  "license": "MIT",
  "keywords": [
    "claude-code",
    "claude",
    "ai",
    "development",
    "automation",
    "commands"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulDuvall/claude-code.git"
  },
  "bugs": {
    "url": "https://github.com/PaulDuvall/claude-code/issues"
  },
  "homepage": "https://github.com/PaulDuvall/claude-code#readme",
  "bin": {
    "claude-commands": "./bin/claude-commands"
